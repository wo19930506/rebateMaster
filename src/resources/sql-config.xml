<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
	<settings
		cacheModelsEnabled="false"
		defaultStatementTimeout="15"
		lazyLoadingEnabled="true" 
		maxRequests="512" 
		maxSessions="128"
		maxTransactions="32"
		useStatementNamespaces="false" />
		
	<sqlMap resource="com/edm/entity/mapper/User.xml" />
	<sqlMap resource="com/edm/entity/mapper/Role.xml" />
	<sqlMap resource="com/edm/entity/mapper/RoleMenu.xml" />
	<sqlMap resource="com/edm/entity/mapper/Menu.xml" />
</sqlMapConfig>